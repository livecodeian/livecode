<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="lc-bootstrap-compile"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="lc-bootstrap-compile"
			DisplayName="lc-bootstrap-compile"
			CommandLine="$(SolutionDir)toolchain\vs-rules\bootstrap.bat $(SolutionDir) $(OutDir)"
			Outputs="grammar_full.g"
			AdditionalDependencies="$(SolutionDir)libscript\src\*.mlc"
			FileExtensions="*.ignored"
			ExecutionDescription="lc-bootstrap-compile"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
